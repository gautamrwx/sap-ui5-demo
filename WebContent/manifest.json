{
   "_version":"1.12.0",
   "sap.app":{
      "id":"gautam.app",
      "type":"application",
      "i18n":"i18n/i18n.properties",
      "title":"{{appTitle}}",
      "description":"{{appDescription}}",
      "applicationVersion":{
         "version":"1.0.0"
      }
   },
   "sap.ui":{
      "technology":"UI5",
      "deviceTypes":{
         "desktop":true,
         "tablet":true,
         "phone":true
      }
   },
   "sap.ui5":{
      "rootView":{
         "viewName":"gautam.app.view.home",
         "type":"XML",
         "async":true,
         "id":"app"
      },
      "dependencies":{
         "minUI5Version":"1.60",
         "libs":{
            "sap.m":{
               
            }
         }
      },
      "models":{
         "i18n":{
            "type":"sap.ui.model.resource.ResourceModel",
            "settings":{
               "bundleName":"gautam.app.i18n.i18n"
            }
         }
      },
      "routing":{
         "config":{
            "routerClass":"sap.m.routing.Router",
            "viewType":"XML",
            "viewPath":"gautam.app.view",
            "controlId":"app",
            "controlAggregation":"pages",
            "async":true
         },
         "routes":[
            {
               "pattern":"",
               "name":"home",
               "target":"targetHome"
            },
            {
               "pattern":"lists",
               "name":"listPage",
               "target":"targetListPage"
            }
         ],
         "targets":{
            "targetHome":{
               "viewName":"home"
            },
            "targetListPage":{
               "viewName":"lists"
            }
         }
      }
   }
}